import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
    Scanner naming= new Scanner(System.in);
    
    System.out.println("Introduce your name");
    
    String name= naming.nextLine();
    
    for(int i=0;i<10;i++){
      System.out.println("Your name is " + name);
    }
  }
}
